/
&limarea_nml
! dtime_latbc             = 3600          ! 3600.
! init_latbc_from_fg      = .true.                 ! always
! itype_latbc             = 1
! latbc_varnames_map_file = 'map_file.lbc'
! latbc_filename          = 'ilbff<ddhhmmss>'

 itype_latbc      = 1
 nudge_hydro_pres = .true.
 dtime_latbc      = 3600.
