    dbz_meta(32)%station_id  = 10873
!
!========================================================
!
! Radar XVI: TUR Tuerkheim
!
! STATION 16: rs_meta(16) and dbz_meta(16): TUR Tuerkheim volume scan
!
    rs_meta(16)%station_name = 'TUR'
    rs_meta(16)%station_id   = 10832
    rs_meta(16)%scanname     = 'PPI0080'
    rs_meta(16)%lambda       = 0.055
    rs_meta(16)%lat          = 48.58528
    rs_meta(16)%lon          = 9.78278
    rs_meta(16)%alt_msl      = 768.0,
    rs_meta(16)%dt_obs       = 0.0, 1e8, 300.0,
    rs_meta(16)%nra          = 180
    rs_meta(16)%ra_inc       = 1000.0
    rs_meta(16)%nobs_times     = 500,
    rs_meta(16)%dt_obs         = 300.0,
    rs_meta(16)%lobstimes_ovwrt_recalc = .true.,
!
    dbz_meta(16)%station_id  = 10832
!
! STATION 33: rs_meta(33) and dbz_meta(33): TUR Tuerkheim precip scan
!
    rs_meta(33)%station_name = 'TUR'
    rs_meta(33)%station_id   = 10832
    rs_meta(33)%scanname     = 'PRECIP'
    rs_meta(33)%lambda       = 0.055
    rs_meta(33)%lat          = 48.58528
    rs_meta(33)%lon          = 9.78278
    rs_meta(33)%alt_msl      = 768.0,
    rs_meta(33)%dt_obs       = 0.0, 1e8, 300.0,
    rs_meta(33)%nel          = 1,
    rs_meta(33)%el_arr       = 0.8,
    rs_meta(33)%nra          = 150
    rs_meta(33)%ra_inc       = 1000.0
    rs_meta(33)%nobs_times     = 500,
    rs_meta(33)%dt_obs         = 300.0,
    rs_meta(33)%lobstimes_ovwrt_recalc = .true.,
!
    dbz_meta(33)%station_id  = 10832
!
!========================================================
!
! Radar XVII: UMD Ummendorf
!
! STATION 17: rs_meta(17) and dbz_meta(17): UMD Ummendorf volume scan
!
    rs_meta(17)%station_name = 'UMD'
    rs_meta(17)%station_id   = 10356
    rs_meta(17)%scanname     = 'PPI0080'
    rs_meta(17)%lambda       = 0.055
