&output_nml
 ! ----------------------------------------------------------------- !
 ! ---  ICON-LAM: output fields - native grid - every 60 Minutes --- !
 ! ----------------------------------------------------------------- !
 filetype             = 2         ! output format: 2=GRIB2, 4=NETCDFv2
 dom                  = 1                         ! write lam domain
 output_time_unit     = 1                         ! 1: seconds
