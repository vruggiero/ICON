    rs_meta(1)%dt_obs         = 300.0,
    rs_meta(1)%lobstimes_ovwrt_recalc = .true.,
!
    dbz_meta(1)%station_id   = 10132  
!
! STATION 18: rs_meta(18) and dbzparams(18): BOO Boosted precip scan
!
    rs_meta(18)%station_name = 'BOO'
    rs_meta(18)%station_id   = 10132
    rs_meta(18)%scanname     = 'PRECIP'
    rs_meta(18)%lambda       = 0.055
